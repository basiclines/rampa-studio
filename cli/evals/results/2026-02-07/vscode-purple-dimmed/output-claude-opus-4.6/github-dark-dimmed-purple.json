  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment", "string.comment"],
      "settings": {
        "foreground": "#768390"
      }
    },
    {
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.other.constant",
        "variable.other.enummember",
        "variable.language"
      ],
      "settings": {
        "foreground": "#ae84ec"
      }
    },
    {
      "scope": ["entity", "entity.name"],
      "settings": {
        "foreground": "#d7c0f2"
      }
    },
    {
      "scope": "variable.parameter.function",
      "settings": {
        "foreground": "#adbac7"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#59cb86"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#dc475a"
      }
    },
    {
      "scope": ["storage", "storage.type"],
      "settings": {
        "foreground": "#dc475a"
      }
    },
    {
      "scope": ["storage.modifier.package", "storage.modifier.import", "storage.type.java"],
      "settings": {
        "foreground": "#adbac7"
      }
    },
    {
      "scope": [
        "string",
        "punctuation.definition.string",
        "string punctuation.section.embedded source"
      ],
      "settings": {
        "foreground": "#c3a2ee"
      }
    },
    {
      "scope": "support",
      "settings": {
        "foreground": "#ae84ec"
      }
    },
    {
      "scope": "meta.property-name",
      "settings": {
        "foreground": "#ae84ec"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#dc7e47"
      }
    },
    {
      "scope": "variable.other",
      "settings": {
        "foreground": "#adbac7"
      }
    },
    {
      "scope": "invalid.broken",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#dc475a"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#dc475a"
      }
    },
    {
      "scope": "invalid.illegal",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#dc475a"
      }
    },
    {
      "scope": "invalid.unimplemented",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#dc475a"
      }
    },
    {
      "scope": "carriage-return",
      "settings": {
        "fontStyle": "italic underline",
        "background": "#dc475a",
        "foreground": "#f3eef6",
        "content": "^M"
      }
    },
    {
      "scope": "message.error",
      "settings": {
        "foreground": "#dc475a"
      }
    },
    {
      "scope": "string source",
      "settings": {
        "foreground": "#adbac7"
      }
    },
    {
      "scope": "string variable",
      "settings": {
        "foreground": "#ae84ec"
      }
    },
    {
      "scope": ["source.regexp", "string.regexp"],
      "settings": {
        "foreground": "#c3a2ee"
      }
    },
    {
      "scope": [
        "string.regexp.character-class",
        "string.regexp constant.character.escape",
        "string.regexp source.ruby.embedded",
        "string.regexp string.regexp.arbitrary-repitition"
