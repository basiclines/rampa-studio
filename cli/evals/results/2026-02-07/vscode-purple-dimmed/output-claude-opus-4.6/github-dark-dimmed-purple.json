        "foreground": "#636e7b",
        "background": "#ae84ec"
      }
    },
    {
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#d7c0f2",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#ae84ec"
      }
    },
    {
      "scope": "meta.separator",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#9864ec"
      }
    },
    {
      "scope": "meta.output",
      "settings": {
        "foreground": "#ae84ec"
      }
    },
    {
      "scope": [
        "brackethighlighter.tag",
