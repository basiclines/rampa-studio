      ],
      "settings": {
        "foreground": "#dc475a"
      }
    },
    {
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#ae84ec"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#ae84ec"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#ae84ec"
      }
    },
    {
      "scope": "entity.name.tag.css",
      "settings": {
        "foreground": "#59cb86"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#ae84ec"
      }
    },
    {
      "scope": "entity.other.attribute-name.class.css",
      "settings": {
